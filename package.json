{
  "name": "clean-node-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "yarn run test:staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "env": ["jest"]
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3"
  },
  "scripts": {
    "test": "jest --watch --silent --verbose --noStackTrace",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent --noStackTrace"
  }
}
